     type : 34
   -
     code : |1+
      
      if MyChildWindow.Name = WIN_KBCARD_MOBILE.Name then
      	
      	if MyChildWindow.ReturnedValue = True then
      		
      		if WIN_KBCARD_MOBILE.AddMode then
      			
      			// Executes the add card process
      			if KANBAN_TASKS.ExecuteProcess(trtAddKanbanCard, WIN_KBCARD_MOBILE.gCard, WIN_KBCARD_MOBILE.ListCard) = False then
      				return
      			end
      			
      			// Adds the card in the list
      			nIndex is int = WIN_KBCARD_MOBILE.ListCard.AjouteCarte( WIN_KBCARD_MOBILE.gCard )
      			if nIndex > 0 then
      				
      				// Set the position on the new card		
      				WIN_KBCARD_MOBILE.ListCard.PositionListe( WIN_KBCARD_MOBILE.ListCard.Card[ nIndex ] )
      			end
      			
      		else
      			
      			KANBAN_TASKS.Modified = True
      		end
      	end
      end
