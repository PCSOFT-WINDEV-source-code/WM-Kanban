      p_codes :
       -
         internal_properties : CAAAAAgAAABl/LvdRmqVLdDZ6i92CQwDb59iaNu59UyNLmyNXYsmLIdZuXaZL3QgAuDCzjmX/Rc/gNBJFgnK760YGDwr1JiA/mhusiHozQY95kAB8QdaEMr3nsINq1x3ac7agh/BWOD8hif4F2l44UHzVHnJTGJY8Y9zlaixMhIPiJidsiFWaPCo9hM0i0ykwP/xAeC/Csk=
         code : |1+
          
          
          // If the current Kanban has not been saved
          if KanbanFile.Value = "" then
          	
          	// If the current Kanban has been modified
          	if KANBAN_TASKS.Modified = True then
          		
          		if not ( LIST_PENDING.Card.Empty _and_ LIST_INPROGRESS.Card.Empty _and_ LIST_DONE.Card.Empty ) then
          			
          			// Requests save confirmation
          			switch Confirm(Cancel, "Some changes have not been saved. Save Kanban now?")
          			
          				// Saves
          				case Yes
          					
          					// If unable to save, cancel
          					OPT_SAVE.ExecuteProcess(trtMenuOption)
          					
          				// Cancels
          				case Cancel
          					SetFocusAndReturnToUserInput()
          					
          				// Continues
          				case No
          			end
          		end
          		
          	end
          end
          
          // Deletes all cards
          KANBAN_TASKS.DeleteAllCard()
          
          // Reset
          KANBAN_TASKS.Modified = False
          KanbanFile.Reset()
         type : 32
   -
     name : OPT_OPEN
     identifier : 0x2389ec770d0add83
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAADUUDW1FVADYhiJlPrlJU7uWnIAtlZlBzfUYx0PQYE7a+x/JsKEgXRcgJiE1FwPtw71G4zL4nauTPaR4SnauQZda38vCcInb8qnt4Q0OPDOuZszgZYie81kAe5Ftv0M3BW5+9oVQgM6wgg7yGzDa6h32sBsVI3fwHViOyJV6vI0N3IC1rNeNR7ANG5UEm43Mu4hNxB/ImCgsYhuUnBjhvdUUvgomKuIntmDTiI+D/6Xy8R/1RurMA==
         code : |1+
          
          URISelect( procedure( bSuccess is boolean, File is URI){
          	
          	if bSuccess then
          		
          		// Start on a blank Kanban
          		OPT_NEW.ExecuteProcess(trtMenuOption)
          		
          		// Stores
          		KanbanFile = File
          		
          		// Loads and deserializes
          		URIToBuffer( KanbanFile, procedure( bResult is boolean, bufKanban is buffer, sFormat is string) {
          			
          			when exception in
          				
          				jsKanbanDescription	is JSON = bufKanban
          				
          				if jsKanbanDescription["ListPending"]..Exist then
          					// Add the cards in the Kanban
          					for each jsCard of jsKanbanDescription["ListPending"]
          						LIST_PENDING.AddCard(JSONTokbCard( jsCard ))
          					end
          				end
          				
          				if jsKanbanDescription["ListInProgress"]..Exist then
          					// Add the cards in the Kanban
          					for each jsCard of jsKanbanDescription["ListInProgress"]
          						LIST_INPROGRESS.AddCard(JSONTokbCard( jsCard ))
          					end
          				end
          				
          				if jsKanbanDescription["ListDone"]..Exist then
          					// Add the cards in the Kanban
          					for each jsCard of jsKanbanDescription["ListDone"]
          						LIST_DONE.AddCard(JSONTokbCard( jsCard ))
          					end
          				end
          				
          			do
          				Error("The kanban could not be loaded with the stored data")
          				return
          			end
          			
          		} )
          		
          	end
          	
          } )
         type : 32
   -
     name : OPT_SAVE
     identifier : 0x2389ec770d0bdd83
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAAB9NXbCgrHiH0ofPJRjXCAwkYPqz9Ochm7byfUZcHpWlGhVsIwBsCNx4P8W2BMClifItxzt4/N5s4GMx3w4M59/fqN0CmuHjzu4kDvLZM6uzXBcrlqGSYu4xhB8YV7CrkhWuXMjb/O5dtdgPnoZ8wgERBluBl5vQfx+HH+BI7DK
         code : |1+
          
          // If the Kanban is empty
          if LIST_PENDING.Card.Empty _and_ LIST_INPROGRESS.Card.Empty _and_ LIST_DONE.Card.Empty then
          	
          	// If on a new Kanban
          	if KanbanFile.Value = "" then
          		SetFocusAndReturnToUserInput(KANBAN_TASKS)
          	end	
          end
          
          // If there is no file
          if KanbanFile.Value = "" then	
          	URISelect(Save, "", uriCreate) 	
          end
          
          Save( True, KanbanFile )
          
          	INTERNAL PROCEDURE Save( bSuccess is boolean, File is URI )
          		
          		// If selected successfully
          		if bSuccess then
          			
          			// Stores 
          			KanbanFile = File
          			
          			jsKanbanDescription is JSON
          			
          			for each Card of LIST_PENDING.Card
          				Add(jsKanbanDescription.ListPending, kbCardToJSON( Card )) 
          			end
          			
          			for each Card of LIST_INPROGRESS.Card
          				Add(jsKanbanDescription.ListInProgress, kbCardToJSON( Card )) 
          			end
          			
          			for each Card of LIST_DONE.Card
          				Add(jsKanbanDescription.ListDone, kbCardToJSON( Card )) 
          			end
          			
          			// Serializes and saves to disk
          			BufferToURI(jsKanbanDescription..JSONFormat, KanbanFile, uriCreate)
          			
          		end
          	end
          
         type : 32
