   -
     name : DragCard_Drop
     procedure_id : 1407916371569995387
     type_code : 14
     code : |1+
      procedure DragCard_Drop(Card ) //is kbCard)
      
      // execute add and cancel process if False is returned
      if ExecuteProcess(KanbanControl, trtDeleteKanbanCard, Card) = False then
      	// cancel
      	return
      end
      
      // Delete card
      nIndex is int =  Card.Index
      ListCard.SupprimeCarte(nIndex) 
      cardCopy	is kbCard	= Card	
      // Display a toast to allow the user to undo deletion of the card
      toast		is Toast
      
      <if ExecutionTarget = Android or ExecutionTarget = iOS>		
      	toast.Text = <§R13502efd0a226952¤en-GB¤Card deleted.§>
      <else>	
      	toast.Title			= Card..Title
      	toast.Text			= <§R13501804036acbb1¤en-GB¤was deleted§>
      	toast.Overlayable	= False
      	toast.CloseOnClick	= True
      <end>
      
      toast.DisplayDuration			= 5 s
      toast..Button[1]..Caption		= <§R134ed795566f1863¤en-GB¤Undo§>
      toast..Button[1]..ActionClick	= () => { ListCard.InsèreCarte( nIndex, cardCopy ) }
      toast.Display()
     type : 458752
